#!/bin/bash

checkExitCode() {
if [ $? -ne 0 ]; then
    echo "Error"
    exit 1;
fi
}

BASEDIR=`dirname $0`

. $BASEDIR/io.sh 11 0 4 "$@"

FILENAME=${INPUTS3%.nam}
cd "${INPUTS3%/*}"
mf2005 $INPUTS3 >> output.log
mv $FILENAME.cbb $OUTPUTS2
mv $FILENAME.ddn $OUTPUTS3
mv $FILENAME.hds $OUTPUTS4
mv $FILENAME.lst $OUTPUTS1
FILE=GLOBAL.DAT     
if [ -f $FILE ]; then
mv GLOBAL GLOBAL $OUTPUTS5
fi

checkExitCode
